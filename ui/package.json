{
  "name": "league-of-experts-ui",
  "version": "0.1.0",
  "description": "Cross-platform UI for League of Experts - Multi-LLM Orchestration System",
  "author": {
    "name": "Carlos Mundim",
    "email": "carlos.mundim@outlook.com"
  },
  "contributors": [
    {
      "name": "KODA (Tiger)",
      "email": "tiger@solunai.co.jp"
    },
    {
      "name": "Jeanette Dennisson"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/CarlosMundim/league-of-experts.git"
  },
  "main": "electron/main.ts",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && electron .\"",
    "electron:build": "npm run build && electron-builder",
    "electron:build:win": "npm run build && electron-builder --win",
    "electron:build:mac": "npm run build && electron-builder --mac",
    "electron:build:linux": "npm run build && electron-builder --linux"
  },
  "dependencies": {
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-query": "^3.39.0",
    "zustand": "^4.5.0",
    "react-markdown": "^9.0.0",
    "lucide-react": "^0.400.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.0",
    "concurrently": "^8.2.0",
    "electron": "^30.0.0",
    "electron-builder": "^24.13.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.4.0",
    "vite": "^5.4.0",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.solunai.league-of-experts",
    "productName": "League of Experts",
    "directories": {
      "output": "dist"
    },
    "files": [
      "dist/**/*",
      "electron/**/*",
      "package.json"
    ],
    "win": {
      "target": ["nsis", "portable"],
      "icon": "build/icons/icon.ico"
    },
    "mac": {
      "target": ["dmg", "zip"],
      "icon": "build/icons/icon.icns",
      "category": "public.app-category.productivity"
    },
    "linux": {
      "target": ["AppImage", "deb"],
      "icon": "build/icons",
      "category": "Office"
    }
  },
  "license": "MIT"
}
